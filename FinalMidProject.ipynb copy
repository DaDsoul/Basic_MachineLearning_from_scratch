   "source": [
    "def error_estimate(W, X, Y):\n",
    "    error = 0\n",
    "    index = 0\n",
    "    for r in X:\n",
    "        if predict(r, W) != sign(Y[index]):\n",
    "            error += 1\n",
    "        index += 1\n",
    "    error /= Y.shape[0]\n",
    "    return error"
   ]
  },
  {
