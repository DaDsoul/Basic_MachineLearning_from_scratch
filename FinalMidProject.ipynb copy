   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "def train_lin_reg(X, Y):\n",
    "    X_pseudo = np.dot(inv(np.dot(X.T, X)), X.T)\n",
    "    w = np.dot(X_pseudo, Y) \n",
    "    return w\n",
    "\n",
    "def error_lin_reg(X, Y, W):\n",
    "    size = X.shape[0]\n",
    "    error = 0\n",
    "    index = 0\n",
    "    for data in X:\n",
    "        error += math.pow((np.dot(W.T, X[index]) - Y[index]), 2)\n",
    "        index += 1\n",
    "    error/=size\n",
    "    return error"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "W_lin = train_lin_reg(X, Y_f)\n"
   ]
